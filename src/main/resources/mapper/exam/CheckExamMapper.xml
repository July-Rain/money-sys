<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lawschool.dao.exam.CheckExamDao">
    <select id="getUserExamIdByCheckUserId" resultType="string">
        select user_exam_id from
        law_check_exam
        where check_EXAM_USER_ID =#{checkUserId}
    </select>

</mapper>